  "dependencies": {
    "@justeat/f-dom": "1.1.0",
    "@justeat/f-logger": "0.8.1",
    "@justeat/f-utils": "0.3.0",
    "@justeat/fozzie-colour-palette": "3.0.0-beta.4",
    "fontfaceobserver": "2.1.0",
    "include-media": "1.4.9",
    "kickoff-grid.css": "1.2.0",
    "normalize-scss": "7.0.1"
  },
  "devDependencies": {
    "@justeat/browserslist-config-fozzie": "1.1.1",
    "@justeat/gulp-build-fozzie": "10.2.1",
    "@justeat/js-test-buddy": "0.4.1",
    "concurrently": "5.2.0",
    "coveralls": "3.1.0",
    "danger": "10.2.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "keywords": [
    "fozzie",
    "eyeglass-module"
  ],
  "eyeglass": {
    "name": "fozzie",
    "sassDir": "src/scss",
    "needs": "^1.1.2",
    "exports": false
  },
  "scripts": {
    "compile": "babel -d dist src --ignore \"src/**/*.test.js\"",
    "lint": "yarn run lint:css && yarn run lint:js",
    "lint:css": "gulp scss:lint --prod",
    "lint:js": "gulp scripts:lint --prod",
    "prepare": "concurrently -n \"lint,compile,test\" -c \"blue,yellow,green\" \"yarn lint\" \"yarn compile\" \"yarn test\" --kill-others-on-fail",
    "test": "gulp scripts:test",
    "test:cover": "gulp scripts:test:coverage",
    "test:cover:CI": "cat coverage/lcov.info | coveralls"
  },
  "stylelint": {
    "extends": "@justeat/stylelint-config-fozzie"
  },
  "browserslist": [
    "extends @justeat/browserslist-config-fozzie"
  ],
  "assets": [
    {
      "root": "src/",
      "glob": "fonts/**/*"
    }
  ]
}
